{
  "name": "pr-www",
  "version": "1.0.0",
  "description": "Portfolio Report",
  "author": "Thomas Fabritius",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server --watch api",
    "devapi": "cross-env NODE_ENV=production nodemon server/index.js --watch server --watch api",
    "build": "nuxt build",
