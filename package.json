    "axios": "^0.19.0",
    "connect-session-sequelize": "^6.0.0",
    "cross-env": "^6.0.3",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.3.0",
    "fuse.js": "^3.4.6",
    "ip2location-nodejs": "^8.3.0",
    "lodash": "^4.17.15",
    "nedb-promises": "^4.0.0",
    "nodemailer": "^6.4.2",
    "nuxt": "^2.11.0",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "validator": "^12.1.0",
    "vue-google-charts": "^0.3.2",
    "xml-js": "^1.6.11"
